<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/listFragment">

    <fragment
        android:id="@+id/parkingFragment"
        android:name="ie.wit.parking.ui.edit.EditFragment"
        android:label="@string/action_donate"
        tools:layout="@layout/fragment_parking" >
        <action
            android:id="@+id/action_donateFragment_to_reportFragment"
            app:destination="@id/listFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/listFragment"
        android:name="ie.wit.parking.ui.list.ListFragment"
        android:label="@string/action_report"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_reportFragment_to_donateFragment"
            app:destination="@id/parkingFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
</navigation>